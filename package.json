  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch -d",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "logs": "npx pm2 logs",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2": "cp .env dist/ && npx pm2 start pmconfig.json"
  },
  "dependencies": {
    "@irisnet/irishub-sdk": "git+https://github.com/hangts/irishub-sdk-js.git#91a833a6f620018369cfae038888a29d874562f9",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "~7.5.0",
    "@nestjs/mongoose": "~7.0.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.5.7",
    "bech32": "^1.1.4",
    "bignumber.js": "^9.0.1",
    "blueimp-md5": "^2.16.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "hex": "^0.1.0",
    "log4js": "^6.1.2",
    "moment": "^2.27.0",
    "mongoose": "~5.9.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "sha256": "^0.2.0",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-md5": "^1.2.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
